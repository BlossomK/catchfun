<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration 
PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.catchmind.catchfun.member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.catchmind.catchfun.board.model.vo.Board" alias="Board" />
		<typeAlias type="com.catchmind.catchfun.board.model.vo.Reply" alias="Reply" />
		<typeAlias type="com.catchmind.catchfun.admin.model.vo.Faq" alias="Faq" />
		<typeAlias type="com.catchmind.catchfun.admin.model.vo.Notice" alias="Notice" />
		<typeAlias type="com.catchmind.catchfun.admin.model.vo.Member" alias="Member1"/>
		<typeAlias type="com.catchmind.catchfun.admin.model.vo.Project" alias="Project1"/>
		<typeAlias type="com.catchmind.catchfun.admin.model.vo.Question" alias="Question" />
	</typeAliases>
	
	
	<mappers>
		<mapper resource="mappers/member-mapper.xml"/>
		<mapper resource="mappers/board-mapper.xml"/>
		<mapper resource="mappers/admin-mapper.xml"/>
		<mapper resource="mappers/admin-mapper2.xml"/>
	</mappers>
	
	
</configuration>


